.slideFadeTop {
    animation-duration: 0.2s;
    animation-timing-function: ease-in-out;
    animation-fill-mode: backwards;
}
.slideFadeTop.ng-enter {
    visibility: hidden;
    animation-name: fadeAndSlideFromTop;
    animation-delay: 0.25s;
    animation-play-state: paused;
}
.slideFadeTop.ng-enter.ng-enter-active {
    visibility: visible;
    animation-play-state: running;
}
.slideFadeTop.ng-leave {
    animation-name: fadeAndSlideToBottom;
    animation-play-state: running;
}
.slideFadeTop.ng-leave.ng-leave-active {
    animation-play-state: running;
}
@keyframes fadeAndSlideFromTop {
    from {
        opacity: 0;
        transform: translateY(-700%);
    }
    to {
        opacity: 0;
    }
}
@keyframes fadeAndSlideToBottom {
    from {
        opacity: 0;
    }
    to {
        opacity: 0;
        transform: translateY(40%);
    }
}

